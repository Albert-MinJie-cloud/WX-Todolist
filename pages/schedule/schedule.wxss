.schedule {
  margin: 0;
  padding: 0;
  height: 100vh;
  background: #f7f8fa;
}

.today {
  text-align: center;
  font-size: 1rem;
  border-top: 1px solid #f7f8fa;
  border-bottom: 1px solid #f7f8fa;
  padding: 10rpx 0;
  background: #f7f8fa;
}

.cell {
  margin-bottom: 10rpx;
}

.van-swipe-cell__left {
  display: inline-block;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #07c160;
}

.van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #ee0a24;
}